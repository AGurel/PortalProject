@model PortalProject.Web.Models.FaqModel;
@{
    ViewBag.Title = "Sıkça Sorulan Sorular | " + @ViewBag.Setting.Title;
}

<!-- MAIN TITLE -->
<div class="main-title">
    <div class="container">
        <h1 class="main-title__primary">Sıkça Sorulan Sorular</h1>
    </div>
</div><!-- /.main-title -->
<!-- BREADCRUMBS -->
<div class="breadcrumbs">
    <div class="container">
        <span>
            <a class="home" href="/" title="Anasayfaya Dön" rel="v:url">Anasayfa</a>
        </span>
        <span>
            <span>Sıkça Sorulan Sorular</span>
        </span>
    </div>
</div><!-- /.breadcrumbs -->

<div class="container">

    <div class="row margin-bottom-60 hentry">

        <div class="col-xs-12  col-md-12">
            @foreach (var item in Model.FaqList)
	        {
                <h3><a href="/sss/@item.SeoUrl/@item.Id" style="color:#444444;">@item.Title</a></h3>
                <p>
                    @item.Content
                </p>
            }         
        </div><!-- /.col -->

    </div><!-- /.row -->

</div><!-- /.container -->

